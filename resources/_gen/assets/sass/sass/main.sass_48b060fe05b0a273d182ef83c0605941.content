.carousel-block{display:flex;flex-direction:column;justify-content:center;height:100%;background-position:50%;background-size:cover;background-image:linear-gradient(0deg,rgba(36,44,51,.5),rgba(36,44,51,.5));align-items:center}.pic{width:50px;height:50px;margin-right:1rem}.latest-news{padding-bottom:50px;width:90%;margin:auto}.latest-news .latest-news-title{margin:3rem 0}.latest-news .information{display:flex;color:#000;text-decoration:none;flex-direction:row;font-size:22px;letter-spacing:2px;margin:10px 0;padding:10px 0;cursor:pointer}.latest-news .information:hover{color:#ff8c33}.latest-news .information .information-start-date{width:100px;background:#ff8c33;color:#f9f6ef;text-align:center;padding:5px;font-size:20px;border-radius:18px;margin:auto}.latest-news .more-news{float:right;font-size:18px;color:#000}